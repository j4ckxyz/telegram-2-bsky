# Copy this file to .env and fill values
# Telegram
TELEGRAM_BOT_TOKEN=
TELEGRAM_ALLOWED_USER_ID=123456789

# Twitter (emusks) - preferred: set TWITTER_AUTH_TOKEN to a valid auth_token cookie value
TWITTER_AUTH_TOKEN=
TWITTER_AUTH_TOKEN_FILE=./data/twitter_auth.json

# Bluesky (ATProto) - either use app password login or OAuth
BLUESKY_SERVICE=https://bsky.social
BLUESKY_IDENTIFIER=your-email-or-handle
BLUESKY_PASSWORD=
BLUESKY_SESSION_FILE=./data/bluesky_session.json

# Nostr
NOSTR_NSEC=
NOSTR_NSEC_FILE=./data/nostr_nsec.txt
NOSTR_RELAYS=wss://relay.damus.io,wss://relay.nostr.info

# General
DATA_DIR=./data
